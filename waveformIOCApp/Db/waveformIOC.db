record(bo, "$(SYS):$(DEV):Reset-CMD")
{
    field(DTYP, "stream")
    field(OUT, "@waveformIOC.proto cmd(*RST) $(PORT)")
    field(PINI, "YES")
}

record(stringin, "$(SYS):$(DEV):DeviceIdentity-RB")
{
    field(DTYP, "stream")
    field(INP, "@waveformIOC.proto getIDN(40) $(PORT)")
    field(PINI, "YES")
}